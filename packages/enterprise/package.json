{
  "name": "@neurectomy/enterprise",
  "version": "0.1.0",
  "description": "Enterprise features for NEURECTOMY: SSO/SAML, Multi-Tenancy, SOC2 Compliance, and Audit Logging",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/index.js",
      "types": "./dist/auth/index.d.ts"
    },
    "./tenancy": {
      "import": "./dist/tenancy/index.js",
      "types": "./dist/tenancy/index.d.ts"
    },
    "./audit": {
      "import": "./dist/audit/index.js",
      "types": "./dist/audit/index.d.ts"
    },
    "./rbac": {
      "import": "./dist/rbac/index.js",
      "types": "./dist/rbac/index.d.ts"
    },
    "./deployment": {
      "import": "./dist/deployment/index.js",
      "types": "./dist/deployment/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@neurectomy/core": "workspace:*",
    "@neurectomy/types": "workspace:*",
    "@neurectomy/legal-fortress": "workspace:*",
    "zod": "^3.22.0",
    "jsonwebtoken": "^9.0.2",
    "jose": "^5.2.0",
    "saml2-js": "^4.0.2",
    "openid-client": "^5.6.4",
    "bcrypt": "^5.1.1",
    "uuid": "^9.0.1",
    "crypto-js": "^4.2.0",
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/crypto-js": "^4.2.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "@neurectomy/core": "workspace:*",
    "@neurectomy/types": "workspace:*"
  },
  "keywords": [
    "neurectomy",
    "enterprise",
    "sso",
    "saml",
    "oauth",
    "multi-tenancy",
    "soc2",
    "compliance",
    "audit-logging",
    "security"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT"
}
