{
  "name": "@neurectomy/legal-fortress",
  "version": "0.1.0",
  "private": true,
  "description": "Legal Fortress - IP Protection, License Compliance, and Plagiarism Detection",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./blockchain": {
      "import": "./dist/blockchain/index.js",
      "types": "./dist/blockchain/index.d.ts"
    },
    "./license": {
      "import": "./dist/license/index.js",
      "types": "./dist/license/index.d.ts"
    },
    "./plagiarism": {
      "import": "./dist/plagiarism/index.js",
      "types": "./dist/plagiarism/index.d.ts"
    },
    "./compliance": {
      "import": "./dist/compliance/index.js",
      "types": "./dist/compliance/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext ts,tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "clean": "rimraf dist .turbo"
  },
  "dependencies": {
    "@neurectomy/core": "workspace:*",
    "@neurectomy/types": "workspace:*",
    "ethers": "^6.9.0",
    "merkletreejs": "^0.3.11",
    "crypto-js": "^4.2.0",
    "spdx-license-list": "^6.8.0",
    "spdx-expression-parse": "^4.0.0",
    "ssri": "^10.0.5",
    "zod": "^3.22.0",
    "uuid": "^9.0.0",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.0",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
