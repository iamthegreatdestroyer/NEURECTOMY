{
  "name": "@neurectomy/performance-engine",
  "version": "0.1.0",
  "description": "Performance optimization, monitoring, and auto-tuning engine for NEURECTOMY platform",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./profiler": {
      "import": "./dist/profiler/index.js",
      "types": "./dist/profiler/index.d.ts"
    },
    "./optimizer": {
      "import": "./dist/optimizer/index.js",
      "types": "./dist/optimizer/index.d.ts"
    },
    "./memory": {
      "import": "./dist/memory/index.js",
      "types": "./dist/memory/index.d.ts"
    },
    "./cache": {
      "import": "./dist/cache/index.js",
      "types": "./dist/cache/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@neurectomy/core": "workspace:*",
    "@neurectomy/types": "workspace:*",
    "zod": "^3.22.0",
    "lru-cache": "^10.1.0",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "neurectomy",
    "performance",
    "optimization",
    "profiling",
    "monitoring",
    "memory",
    "cache",
    "metrics"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/neurectomy/neurectomy.git",
    "directory": "packages/performance-engine"
  }
}
